<Application x:Class="Pos2Web.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Pos2Web"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/themes/Colors.xaml" />
                <ResourceDictionary Source="Resources/themes/Fonts.xaml" />
                <ResourceDictionary Source="Resources/themes/Buttons.xaml" />
                <ResourceDictionary Source="Resources/themes/Text.xaml" />
                <ResourceDictionary Source="Resources/themes/Windows.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
